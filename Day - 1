#include<iostream>
using namespace std;
int main()
{	int day,water;
	cout<<"Enter the No of Days: ";
    cin>>day;
    for(int I=1;I<=day;I++) {
    	cout<<"\nEnter the amount of water for Day"<<I<<": ";
        cin>>water;
        if(water >= 2000){
        	cout<<"YES."<<endl;
        }
        else{
        	cout<<"NO."<<endl;
        }
    }
    return 0;
}
