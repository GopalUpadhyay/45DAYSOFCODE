#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T, X, Y, Z;
	cin>>T;
	while(T--){
	    cin>>X>>Y>>Z;
	    X = abs(X);
	    Y = abs(Y);
	    Z = abs(Z);
	    if((X+Y-Z == 0 || X+Z-Y == 0 || Y+Z-X ==
	    0)){
	        cout<<"yes"<<endl;
	    }
	    else{
	        cout<<"no"<<endl;
	    }
	}
	return 0;
}
