#include <iostream>
using namespace std;

int main() {
	// your code goes here
	double a,b;
	cin>>a>>b;
	char c;
	cin>>c;
	
	if(c == '+'){
	     cout<<fixed<<a+b<<endl;
	}
	else if(c == '-' ){
	    cout<<fixed<<a-b<<endl;
	}
	else if(c == '*'){
	    cout<<fixed<<a*b<<endl;
	}
	else if(b== 0){
	    cout<<"undefined"<<endl;
	    return 0;
	}
	else if(c == '/'){
	    cout<<fixed<<a/b<<endl;
	}
	else{
	    cout<<"invalid operator"<<endl;
	}
	
	
	return 0;
}
 
