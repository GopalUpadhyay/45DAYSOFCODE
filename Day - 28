#include <iostream>
#include <string.h>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	while(T--){
	    int N,zero=0;
	    cin>>N;
	    string ch;
	   cin>>ch;
	    for(int i=0;i<N;i++){
	        if(ch[i] == '0'){
	          zero++;  
	        }
	    }
        float x = ((float)(120-zero)/120)*100;
        
	    if(x >= 75){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
