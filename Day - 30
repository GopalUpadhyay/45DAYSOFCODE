#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	while(T--){
	    int R1,w1,c1,R2,w2,c2,A=0,B=0;
	    cin>>R1>>w1>>c1;
	    cin>>R2>>w2>>c2;
	    if(R1 > R2){
	        A++;
	    }
	    else{
	        B++;
	    }
	    
	    if(w1>w2){
	        A++;
	    }
	    else{
	       B++;
	    } 
	    
	    if(c1>c2){
	        A++;
	    }
	    else{
	       B++;
	    }
	    
	    if(A>B){
	        cout<<"A"<<endl;
	    }
	    else{
	        cout<<"B"<<endl;
	    }
	}
	return 0;
}
