#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	 cin>>T;
	 while(T--){
	     int N,A,B,cntA=0,cntB=0;
	     cin>>N>>A>>B;
	     string s;
	     cin>>s;
	     
	     for(int i=0;i<N;i++){
	         if(s[i] == '0'){
	             cntA++;
	         }
	         else{
	             cntB++;
	         }
	     }
	     cout<<(cntA*A)+(cntB*B)<<endl;
	 }
	return 0;
}
